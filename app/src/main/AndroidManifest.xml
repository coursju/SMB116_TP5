<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smb116.tp5">

    <permission android:name="com.smb116.tp5.provider"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TP5">


        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowDatabase" />
        <provider
            android:name=".provider.MyContentProvider"
            android:authorities="com.smb116.tp5.provider.MyContentProvider"
            android:permission="com.smb116.tp5.provider"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>